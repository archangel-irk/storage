{
  "bitwise": true,
  "camelcase": true,
  "curly": true,
  "forin": true,
  "freeze": true,
  "immed": true,
  "indent": 2,
  "latedef": "nofunc",
  "newcap": true,
  "noarg": true,
  "noempty": true,
  "nonbsp": true,
  "nonew": true,
  "quotmark": "single",
  "undef": true,
  "unused": true,
  "strict": true,
  "maxparams": 3,
  "maxdepth": 2,
  "maxstatements": 3,
  "maxcomplexity": 5,
  "maxlen": 120,

  "laxcomma": true,
  "validthis": true,

  "browserify": true,

  "globals": {
    "jQuery": false,
    "_": false
  }
}