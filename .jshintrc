{
  "bitwise": true,
  "camelcase": true,
  "eqeqeq": true,
  "forin": true,
  "freeze": true,
  "immed": true,
  "indent": 2,
  "latedef": "nofunc",
  "newcap": true,
  "noarg": true,
  "noempty": true,
  "nonbsp": true,
  "nonew": true,
  "quotmark": "single",
  "undef": true,
  "unused": true,
  "strict": true,
  "maxparams": 4,
  "maxdepth": 4,
  "maxstatements": 15,
  "maxcomplexity": 10,
  "maxlen": 120,

  "eqnull": true,
  "expr": true,
  "laxbreak": true,
  "laxcomma": true,
  "validthis": true,

  "browser": true,
  "browserify": true,

  "globals": {
    "_": false,
    "storage": true,

    "describe": false,
    "it": false,
    "assert": false
  }
}